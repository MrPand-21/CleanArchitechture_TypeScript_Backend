{
  "name": "zort",
  "version": "0.0.0",
  "description": "",
  "main": "main.ts",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "build": "./scripts/build.sh",
    "copy:local-env": "cp ./env/local.app.env ./dist/.env",
    "resetDist": "rm -rf ./dist/*",
    "start": "node ./dist/Main.js",
    "start:dev:hot": "webpack-dev-server --hot",
    "start:dev": "./scripts/nest.sh",
    "start:local": "npm run copy:local-env && cd ./dist && node -r dotenv/config Main.js",
    "test": "jest --config jest.json",
    "test:cov": "jest --config jest.json --coverage",
    "lib:check": "ncu",
    "lib:upgrade": "ncu -u && npm install",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "eslint --fix '{src,test}/**/*.ts'",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -n",
    "migration:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/node": "^14.18.26",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/validator": "^13.7.10",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "gts": "^3.1.0",
    "typeorm": "^0.3.9",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "mysql2": "^2.3.3",
    "nestjs": "0.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rxjs": "^7.8.0",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  }
